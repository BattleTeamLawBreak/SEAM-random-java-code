class HelloWorld {

    public static void main(String[] args) {
    
    liftOff(100);


    }
    
    
    public static void liftOff(int countdown)
    {
     System.out.println("countdown starting");
        for(int x = countdown; x >= 0; x--){
            System.out.println(x);
        } System.out.println("countdown ended"); 
    }
}
