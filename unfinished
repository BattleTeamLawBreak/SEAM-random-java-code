class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Rocket Bunny");
         System.out.println(sumFromTo(4 , 7));
         System.out.println(sumFromTo(-3 , 1));
         System.out.println(sumFromTo(7 , 4));
         System.out.println(sumFromTo(9 , 9));
         
    } 
     public static int sumFromTo(int start, int end) {
         int sum = 0;
         for (int x = start; x <= end; x++) {
             sum = sum + x;
         }
         return sum;
         
     }
}
